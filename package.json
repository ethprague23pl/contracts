    "compile": "hardhat compile",
    "deploy": "hardhat deploy-zksync --script deploy-account.ts",
    "deploy-paymaster": "hardhat deploy-zksync --script deploy-paymaster.ts",
    "deploy-event": "hardhat deploy-zksync --script deploy-event.ts",
